    "test-client": "karma start",
    "test-jsdom": "mocha -r babel-register -r jsdom.conf.js -r babel-polyfill test/client.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/vanwagonet/middle-router.git"
